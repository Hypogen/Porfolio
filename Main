import javax.swing.*;
import java.awt.*;

        
public class Main {
    
    public static void main(String[] args) {
        
         JFrame window = new JFrame();
        window.setMinimumSize(new Dimension(1000, 1000));
        window.setLocationRelativeTo(null);
        
        window.setVisible(true);
    }
    
}



